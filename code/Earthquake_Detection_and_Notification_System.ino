void alarm() {
    int value1 = analogRead(xPin); // reading x out
    int value2 = analogRead(yPin); // reading y out
    int value3 = analogRead(zPin); // reading z out

    int xValue = xsample - value1; // finding change in x
    int yValue = ysample - value2; // finding change in y
    int zValue = zsample - value3; // finding change in z

    // Check if earthquake conditions are met
    bool earthquakeDetected = (xValue < minVal || xValue > maxVal || yValue < minVal || yValue > maxVal || zValue < minVal || zValue > maxVal);

    if (earthquakeDetected) {
        consecutiveEarthquakeReadings++;
        if (consecutiveEarthquakeReadings >= requiredConsecutiveReadings) {
            earthquakeState = true;
            consecutiveEarthquakeReadings = 0;
        }
    } else {
        consecutiveEarthquakeReadings = 0;
    }

    if (earthquakeState) {
        if (!isPlaying) {
            myDFPlayer.volume(30); // volume (0~30).
            myDFPlayer.loop(1);    // play the first audio file in loop mode
            isPlaying = true;
            Serial.println("earthquake");
            digitalWrite(ALARM, HIGH);
            sending_animation();
            sendMessage("URGENT: Earthquake Alert! Seek shelter immediately! Move to a safe place away from windows, shelves, and heavy objects that could fall. Stay safe!");
            sendMessage("URGENT: Earthquake Alert! Seek shelter immediately! Perform DUCK, COVER, AND HOLD!!");
            sendMessage("THIS IS AN EMERGENCY!!!");
        }
    } else {
        if (isPlaying) {
            myDFPlayer.stop(); // stop playing audio
            isPlaying = false;
            Serial.println("no earthquake");
        }
    }

    // If the button is pressed, stop the audio
    button = digitalRead(BUTTON_PIN);
    if (button == LOW) {
        myDFPlayer.stop(); // stop playing audio
        myDFPlayer.volume(30); // volume (0~30).
        myDFPlayer.loop(2); 
        digitalWrite(ALARM, LOW); // turn off the alarm LED
        earthquakeState = false; // reset earthquake state
        button = HIGH;
        if (button == LOW){
            myDFPlayer.stop();
            digitalWrite(ALARM, LOW);
            isPlaying = false; // reset the playing flag
        }
    }
}

