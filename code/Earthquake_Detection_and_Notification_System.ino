unsigned long lastMessageCheck = 0;
const unsigned long messageCheckInterval = 10000; // Check for new messages every 10 seconds

void setup()
{
    Serial.begin(115200);
    SIM800.begin(9600, SERIAL_8N1, RX_PIN, TX_PIN);

    // Initialize OLED display
    if (!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)){
        Serial.println(F("SSD1306 allocation failed"));
        for (;;);
    }
    display.display();
    delay(2000); // Pause for 2 seconds

    display.clearDisplay();

    mySerial.begin(9600, SERIAL_8N1, 16, 17); // Initialize Serial2 of tx rx

    pinMode(busyPin, INPUT);

    Serial.println();
    Serial.println(F("Initializing DFPlayer..."));

    if (!myDFPlayer.begin(mySerial))
    { // Use softwareSerial to communicate with mp3.
        Serial.println(F("Unable to begin:"));
        Serial.println(F("1. Please recheck the connection!"));
        Serial.println(F("2. Please insert the SD card!"));
        while (true)
            ;
    }

    Serial.println(F("DFPlayer Mini online."));

    loading();

    for (int i = 0; i < samples; i++) // taking samples for calibration
    {
        xsample += analogRead(xPin);
        ysample += analogRead(yPin);
        zsample += analogRead(zPin);
    }

    xsample /= samples; // taking avg for x
    ysample /= samples; // taking avg for y
    zsample /= samples; // taking avg for z

    pinMode(BUTTON_PIN, INPUT_PULLUP);
    pinMode(POWER, OUTPUT);
    pinMode(ALARM, OUTPUT);

    WiFi.begin(ssid, password);
    while (WiFi.status() != WL_CONNECTED) {
      delay(1000);
      Serial.println("Connecting to WiFi...");
    }
    Serial.println("Connected to WiFi");
    client.setInsecure();
    digitalWrite(POWER, HIGH);
}

void loop() {
    seismograph();
    alarm();

    unsigned long currentMillis = millis();
    if (currentMillis - lastMessageCheck >= messageCheckInterval) {
        lastMessageCheck = currentMillis;
        reply();
    }
}

void seismograph(){
    static unsigned long lastUpdate = 0;
    unsigned long currentMillis = millis();

    if (currentMillis - lastUpdate >= 100) { // Adjust the update interval as needed
        lastUpdate = currentMillis;

        // Read accelerometer values
        int xValue = analogRead(xPin);
        int yValue = analogRead(yPin);
        int zValue = analogRead(zPin);

        // Map the raw values to the display height
        int xMapped = map(xValue, 0, 4095, 0, SCREEN_HEIGHT - 1);
        int yMapped = map(yValue, 0, 4095, 0, SCREEN_HEIGHT - 1);
        int zMapped = map(zValue, 0, 4095, 0, SCREEN_HEIGHT - 1);

        // Shift the data in the buffers to make room for the new values
        for (int i = SCREEN_WIDTH - 1; i > 0; i--)
        {
            xData[i] = xData[i - 1];
            yData[i] = yData[i - 1];
            zData[i] = zData[i - 1];
        }
        xData[0] = xMapped;
        yData[0] = yMapped;
        zData[0] = zMapped;

        // Clear the display and draw the graphs
        display.clearDisplay();

        // x-axis graph
        for (int i = 1; i < SCREEN_WIDTH; i++)
        {
            display.drawLine(i - 1 + 10, SCREEN_HEIGHT - xData[i - 1] - 29, i + 10, SCREEN_HEIGHT - xData[i] - 29, SSD1306_WHITE);
        }

        // y-axis graph
        for (int i = 1; i < SCREEN_WIDTH; i++)
        {
            display.drawLine(i - 1 + 10, SCREEN_HEIGHT - yData[i - 1] - 5, i + 10, SCREEN_HEIGHT - yData[i] - 5, SSD1306_WHITE);
        }

        // z-axis graph
        for (int i = 1; i < SCREEN_WIDTH; i++)
        {
            display.drawLine(i - 1 + 10, SCREEN_HEIGHT - zData[i - 1] + 25, i + 10, SCREEN_HEIGHT - zData[i] + 25, SSD1306_WHITE);
        }

        // x y z labels
        display.setCursor(0, 5);
        display.setTextSize(1);
        display.setTextColor(SSD1306_WHITE);
        display.print("X");
        display.setCursor(0, 27);
        display.print("Y");
        display.setCursor(0, 52);
        display.print("Z");

        display.display();
    }
}

