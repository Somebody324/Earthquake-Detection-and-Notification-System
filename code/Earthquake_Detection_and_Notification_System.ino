void sendMessage(String message) {
  if(WiFi.status() == WL_CONNECTED) {
    for (int i = 0; i < numChatIDs; i++) {
      HTTPClient http;
      String url = "https://api.telegram.org/bot" + botToken + "/sendMessage?chat_id=" + chatIDs[i] + "&text=" + message;

      http.begin(url);
      int httpResponseCode = http.GET();

      if (httpResponseCode > 0) {
        String response = http.getString();
        Serial.println(httpResponseCode);
        Serial.println(response);
      } else {
        Serial.print("Error on sending POST: ");
        Serial.println(httpResponseCode);
      }

      http.end();
    }
  } else {
    Serial.println("WiFi Disconnected");
  }
}

void handleNewMessages(int numNewMessages) {
    Serial.print("Handling ");
    Serial.print(numNewMessages);
    Serial.println(" new messages");
    
    for (int i = 0; i < numNewMessages; i++) {
        String chat_id = String(bot.messages[i].chat_id);
        String text = bot.messages[i].text;

        Serial.print("Received message from chat_id: ");
        Serial.println(chat_id);
        Serial.print("Message text: ");
        Serial.println(text);

        if (text == "/start") {
            bot.sendMessage(chat_id, "Welcome to the Earthquake Alert Bot! Stay safe with real-time earthquake updates and safety tips.", "");
        }
        if (text == "/safetytips") {
            bot.sendMessage(chat_id, "Earthquake Safety Tips:\n1. Drop, Cover, Hold On: Drop to your hands and knees, cover your head and neck, and hold on until shaking stops.\n2. Stay Indoors: Move away from windows and heavy objects. Take shelter under sturdy furniture.\n3. If Outside: Move to an open area away from buildings, trees, and power lines.\n4. Avoid Elevators: Use stairs if you need to evacuate.\n5. Stay Calm: Keep calm and reassure others around you.", "");
        }
        if (text == "/stop_all") {
            bot.sendMessage(chat_id, "The all alarms has been stopped. The system will continue to monitor for new earthquakes.", "");
            myDFPlayer.stop();
            isPlaying = false;
            digitalWrite(ALARM, LOW);
            earthquakeState = false; // Ensure earthquakeState is reset
        }
        if (text == "/stop1") {
            bot.sendMessage(chat_id, "First alarm has been stopped. Now playing Alarm2", "");
            myDFPlayer.stop();
            myDFPlayer.volume(30); // volume (0~30).
            myDFPlayer.loop(2); 
            digitalWrite(ALARM, LOW);
            earthquakeState = false; // Ensure earthquakeState is reset
        }
        if (text == "/stop2") {
            bot.sendMessage(chat_id, "Alarm2 has been stopped.", "");
            myDFPlayer.stop();
            isPlaying = false;
            digitalWrite(ALARM, LOW);
            earthquakeState = false; // Ensure earthquakeState is reset
        }
        if (text == "taenamo") {
            bot.sendMessage(chat_id, "Taenamo rin!!!", "");
        }
        if (text == "/play_alarm1") {
            bot.sendMessage(chat_id, "First alarm is playing", "");
            myDFPlayer.volume(30); // volume (0~30).
            myDFPlayer.loop(1); 
            digitalWrite(ALARM, LOW);
            earthquakeState = false;
        }
    }
}

void reply() {
    if (WiFi.status() == WL_CONNECTED) {
        int numNewMessages = bot.getUpdates(bot.last_message_received + 1);
        if (numNewMessages > 0) {
            handleNewMessages(numNewMessages);
        }
    } else {
        Serial.println("WiFi Disconnected");
    }
}
